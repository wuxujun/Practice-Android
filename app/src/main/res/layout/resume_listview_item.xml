<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" android:layout_margin="@dimen/dimen_10" android:orientation="vertical">
        <ImageView
            android:layout_width="wrap_content" android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content" android:src="@drawable/ic_launcher"/>
        <TextView android:id="@+id/tvItemTitle"
            android:layout_width="match_parent"  android:text="11"
            android:layout_height="wrap_content" style="@style/App_Resume_Item_Title" />
    </LinearLayout>
</LinearLayout>